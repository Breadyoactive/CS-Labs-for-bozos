import java.util.Stack;
import java.io.File;
import java.io.IOException;
import java.util.Scanner;


public class PreFix {
    private Stack<Double> ansGet;
    private double ans;
    private String string;

    public PreFix(String line){
        string= line;
        String[] listOfStuff = line.split(" ");
        ansGet = new Stack<Double>();
        answer(listOfStuff);
    }

    public double answer(String[] x) {
        for(int i = x.length - 1; i >=0; i--){
            if(x[i].equals("+")){
                double one = ansGet.pop();
                double two = ansGet.pop();
                ansGet.push(one + two);
            }else if(x[i].equals("-")){
                double one = ansGet.pop();
                double two = ansGet.pop();
                ansGet.push(one - two);
            }else if(x[i].equals("/")){
                double one = ansGet.pop();
                double two = ansGet.pop();
                ansGet.push(one / two);
            }else if(x[i].equals("*")){
                double one = ansGet.pop();
                double two = ansGet.pop();
                ansGet.push(one * two);
            }else {
                double num = Double.parseDouble(x[i]);
                ansGet.push(num);
            }
        }
        ans = ansGet.peek();
        return ans;
    }

    public String toString(){
        return string + " = " + ans;
    }

    public static void main(String[] args) throws IOException{
        Scanner scan = new Scanner(new File("PreFix.txt"));

        while(scan.hasNext()){
            PreFix preFix = new PreFix(scan.nextLine());
            System.out.println(preFix.toString());
        }


}


}
